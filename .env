# Full Data API base URL (including /app/<app-id>/endpoint/data/v1)
VITE_MONGODB_DATA_API_URL=https://data.mongodb-api.com/app/<app-id>/endpoint/data/v1
# Data API key
VITE_MONGODB_DATA_API_KEY=<your-data-api-key>
# Data source name (often "Cluster0")
VITE_MONGODB_DATA_SOURCE=Cluster0
# Database and collection
VITE_MONGODB_DATABASE=MailMorph
VITE_MONGODB_COLLECTION=metrics